setup:
    sudo apt install -y gpsd gpsd-clients gpsd-tools
    -sudo systemctl stop gpsd
    sudo cp gpsd.env /etc/default/gpsd
    sudo systemctl reenable gpsd
    sudo systemctl restart gpsd

test:
    validator gpsd.env

dev:
    -sudo systemctl stop gpsd
    sudo gpsd --nowait --foreground /dev/ttyUSB0
